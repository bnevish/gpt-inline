{
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "manifest_version": 3,
  "name": "Historical Data Generator",
  "content_scripts": [
    
    {
      "matches": ["<all_urls>"],
      "js": ["js/starter.js"],
      "css": []
    }
  ],
  "icons": {
    "128": "img/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["/css/*", "/pages/*"],
      "matches": [ "<all_urls>" ]
    }
  ],
  "action": {
    "browser_style": true,
    "default_title": "Historical Data Generator",
    "default_popup": "popup.html"
  },
  "version": "0.003"
}