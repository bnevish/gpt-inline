{
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "manifest_version": 3,
  "name": "AI NoteMaker",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/webcomponents-sd-ce.js","js/jquery-3.6.4.min.js", "js/bootstrap.bundle.min.js","gpt-connector.js","common_content.js"],
      "css": ["css/gpt_content.css"]
    }
  ],
  "icons": {
    "128": "img/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["/css/*", "/pages/*"],
      "matches": [ "*://*/*" ]
    }
  ],
  "action": {
    "browser_style": true,
    "default_title": "AI NoteMaker",
    "default_popup": "popup.html"
  },
  "version": "0.003"
}