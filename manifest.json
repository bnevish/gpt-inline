{
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "manifest_version": 3,
  "name": "AI NoteMaker",
  "content_scripts": [
    {
      "matches": ["*://medium.com/*"],
      "js": ["js/webcomponents-sd-ce.js","js/jquery-3.6.4.min.js", "js/bootstrap.bundle.min.js","gpt-connector.js","common_content.js","medium/content.js"],
      "css": ["css/gpt_content.css", "css/bootstrap.min.css", "css/key.css", "css/modals.css"]
    },
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["js/webcomponents-sd-ce.js","js/jquery-3.6.4.min.js", "js/bootstrap.bundle.min.js","gpt-connector.js","common_content.js", "linkedin/content.js"],
      "css": [ "css/gpt_content.css", "css/bootstrap.min.css", "css/key.css", "css/modals.css"]
    },
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["js/webcomponents-sd-ce.js","js/jquery-3.6.4.min.js", "js/bootstrap.bundle.min.js","gpt-connector.js","common_content.js", "twitter/content.js"],
      "css": [ "css/gpt_content.css", "css/bootstrap.min.css", "css/key.css", "css/modals.css"]
    },
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["js/webcomponents-sd-ce.js","js/jquery-3.6.4.min.js", "js/bootstrap.bundle.min.js","gpt-connector.js","common_content.js", "reddit/content.js"],
      "css": [ "css/gpt_content.css", "css/bootstrap.min.css", "css/key.css", "css/modals.css"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["/css/*", "/pages/*"],
      "matches": [ "*://*/*" ]
    }
  ],
  "action": {
    "browser_style": true,
    "default_title": "AI NoteMaker",
    "default_popup": "popup.html"
  },
  "version": "0.001"
}